{
    "name": "dexswap-swapper-contracts-staging-deployer",
    "version": "1.0.0",
    "description": "A simple Hardhat project to deploy the whole DexSwap contract suite with user-chosen parameters, on any given network.",
    "main": "index.js",
    "author": "Agin DropDisco || gin.dropdisco@gmail.com",
    "license": "GPL-3.0",
    "private": true,
    "scripts": {
        "compile": "yarn prepare:core && yarn prepare:periphery && yarn prepare:staking",
        "prepare:core": "mkdirp artifacts && cp dexswap-core/build/*.json artifacts",
        "prepare:periphery": "mkdirp artifacts && cp dexswap-periphery/build/*.json artifacts",
        "prepare:staking": "mkdirp artifacts && cp dexswap-staking/build/*.json artifacts",
        "deploy:rinkeby": "yarn hardhat deploy --network rinkeby  --protocol-fee-native-asset-receiver 0xe6CD7A56F13DcEB1b5ad42B995fAC9eB4a42079b  --protocol-fee-fallback-receiver 0xeED588Fc1A009aFf2f4E90b57D4d74B2F56309c5 --token-registry-address 0x5Ae40444a69a64384158fF7A1C645A997274aE61 --token-registry-list-id 1 --native-asset-wrapper-address 0xc778417E063141139Fce010982780140Aa0cD5Ab --owner-address 0xD3717f94E65AF88F794D7Cf8e33E9F3F8829C223 --deploy-farming",
        "deploy:matic": "yarn hardhat deploy --network matic  --protocol-fee-native-asset-receiver 0x140B8F40701366145d2908b3aC1a88dD0e62870D  --protocol-fee-fallback-receiver 0xeED588Fc1A009aFf2f4E90b57D4d74B2F56309c5 --token-registry-address 0x52d2026723cd76E1Feae2719aa6F56aBe585030E --token-registry-list-id 1 --native-asset-wrapper-address 0xA6FA4fB5f76172d178d61B04b0ecd319C5d1C0aa --owner-address 0x60993D093a39881DbC153312943A6612a1a675b5 --deploy-farming",
        "deploy:moonbeam": "yarn hardhat deploy --network moonbeam  --protocol-fee-native-asset-receiver 0x140B8F40701366145d2908b3aC1a88dD0e62870D  --protocol-fee-fallback-receiver 0xeED588Fc1A009aFf2f4E90b57D4d74B2F56309c5 --token-registry-address 0x853447b84b0A498831E0731Ff5FFc99b6B515A14 --token-registry-list-id 1 --native-asset-wrapper-address 0xe73763DB808ecCDC0E36bC8E32510ED126910394 --owner-address 0xBfBa42de8147de1B20731bD8150531c50cd10803"
    },
    "devDependencies": {
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "@nomiclabs/hardhat-truffle5": "^2.0.0",
        "@nomiclabs/hardhat-web3": "^2.0.0",
        "@typechain/ethers-v5": "^6.0.4",
        "babel-eslint": "^10.1.0",
        "dexswap-staking-distribution": "git://github.com/Agin-DropDisco/dexswap-staking-distribution.git#1.1.0",
        "dotenv": "^8.2.0",
        "eslint": "^7.18.0",
        "hardhat": "^2.0.10",
        "hardhat-typechain": "^0.3.5",
        "husky": "^4.3.8",
        "prettier": "^2.2.1",
        "web3": "^1.3.3"
    },
    "dependencies": {
        "@nomiclabs/buidler": "^1.4.8",
        "@nomiclabs/hardhat-ethers": "^2.0.2",
        "@nomiclabs/hardhat-etherscan": "^2.1.2",
        "solidity-coverage": "^0.7.13",
        "hardhat-dependency-compiler": "^1.1.1"
    }
}
